{
    "name": "thefaizan/FbInsights",
    "description": ":Get facebook fanpages insights",
    "license": "proprietary",
    "authors": [
        {
            "name": "Faizan Afzal",
            "email": "faizan@stallyoons.com",
            "homepage": "https://www.stallyons.com"
        }
    ],
    "homepage": "https://github.com/thefaizan/FbInsights",
    "keywords": ["Laravel", "FbInsights"],
    "require": {
        "illuminate/support": "~5",
        "sammyk/laravel-facebook-sdk": "^3.5",
        "doctrine/dbal": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "SmartHub\\FbInsights\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmartHub\\FbInsights\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SmartHub\\FbInsights\\FbInsightsServiceProvider"
            ],
            "aliases": {
                "FbInsights": "SmartHub\\FbInsights\\Facades\\FbInsights"
            }
        }
    }
}
